{
  "expo": {
    "name": "spotifyndr",
    "slug": "spotifyndr",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "spotifyndr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "res/spotifyndr/images/icon.png",
    "splash": {
      "image": "res/spotifyndr/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1BD760"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
